  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.10/js/i18n/defaults-en_US.min.js" type="be9edb3fae5c0917af8a89f0-text/javascript"></script>
  
<script type="be9edb3fae5c0917af8a89f0-text/javascript">
// set today
var is_today = 1;
// show and hide tooltip
$(document).ready(function() {

	$('[data-toggle="popover"]').popover({
		trigger: 'hover click'
	});

	// walkin load table status for walk-in modal
  var walkin_tables = document.getElementById('walk-in');
  walkin_tables.addEventListener('click', ajaxTableStatus);
  // $('.selectpicker').selectpicker('render');
  // $('.selectpicker').selectpicker('refresh');

  // ajaxTableStatus();

  // walkin load and diplay tables
  function searchSuccess(data) {
   $('#ajax-table-status').html(data).show(500);
   $('#ajax-table-status-loader').hide();
   $.getScript( '/static/js/dash-walkin.min.js' );
  }

  // walkin ajax
  function ajaxTableStatus() {
		$('#ajax-table-status').hide();
    $('#ajax-table-status-loader').show();
    // $('.selectpicker').selectpicker();
    $.ajax({
     type: "POST",
     url: "/dashboard/" + instance_slugname + "/ajax-table-status/",
     data: {
      'restaurant': instance_id,
     },
     success: searchSuccess,
     dataType: 'html'
    });
  }


	// show input field
	$('.timeslot-limit').on('click', function() {
	  var $timeslot_id = $(this).data("timeslot-id");
	  var $currentlimit = $(this).text().trim();
	  $(".timeslot-limit").filter('[data-timeslot-id="'+$timeslot_id+'"]').removeClass('btn-info').addClass('btn-primary');
	  $(".set-limit").slideDown(500);
	  $(".set-limit input").val($currentlimit);
		$("#cancel-update").attr('data-timeslotid', $timeslot_id);
	});

	// hide input field
	$('#cancel-update').on('click', function() {
		var timeslot_id = $("#cancel-update").attr("data-timeslotid");
		$(".timeslot-limit").filter('[data-timeslot-id="'+timeslot_id+'"]').removeClass('btn-primary').addClass('btn-info');
		$("#cancel-update").attr('data-timeslotid',"");
	  $(".set-limit").slideUp(500);
	});

	// update timeslot date
	$(document).on('click', '.update-timeslot-limit', function() {
		var selectedHours = 37175;
		var restaurant = instance_id;
		var timeslotdate = '05/05/2020';
		var csrftoken = $("input[name=csrfmiddlewaretoken]").val();
		var $timeslot_id = $("#cancel-update").attr("data-timeslotid");
	  var $newlimit = $(this).closest(".set-limit").find('input').val();
		// check if valid number
		if(!$newlimit.match(/^\d+$/)) {
	      $(this).text('Please enter number 0-999').addClass('btn-danger');
	      setTimeout(function(){
	          $('.update-timeslot-limit').slideUp(1000).removeClass('btn-danger').slideDown(1000).addClass('btn-primary').text('Save');
	      }, 2000);
	      return
	    }
		$.ajax({
		 type: "PUT",
		 url: "/dashboard/" + instance_slugname + "/ajax-timeslot-date/",
		 headers:{
        "X-CSRFToken": csrftoken
      },
		 data: {
			'hours_id': selectedHours,
			'restaurant': restaurant,
			'timeslotdate': timeslotdate,
			'timeslotlimit': $newlimit,
			'timeslot': $timeslot_id,
		 },
		 success: function(r) {
			 results = r;
			 $(".timeslot-limit").filter('[data-timeslot-id="'+$timeslot_id+'"]').removeClass('btn-primary').addClass('btn-info');
			 $(".timeslot-limit").filter('[data-timeslot-id="'+$timeslot_id+'"]').text($newlimit);
       $(".set-limit").slideUp();
		 },
		 error: function() {
			 alert('There was an error loading your business hours. Please try again!');
			 location.reload();
		 }
		});
	});
	// scroll timeslot to center
  center = document.getElementById("center").scrollIntoView({ inline: "center", behavior: "smooth", block: 'nearest'});
});
</script>

